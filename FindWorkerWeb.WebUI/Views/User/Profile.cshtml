            cv_document_deletePhoto: "Fotoğrafı sil",
            cv_document_uploadPhoto: "Fotoğraf yükle",
            cv_document_changePhoto: "Fotoğrafı değiştir",
            cv_document_cropPhoto: "Fotoğrafı Kırp",
            cv_document_page: "Sayfa",
            cv_document_deleteParagraph: "Paragrafı Sil",
            cv_document_addParagraph: "Paragraf Ekle",
            cv_document_addLine: "Satır ekle",
            cv_document_deleteLine: "Satır sil",
            cv_document_changeType: "Türünü Değiştir",
            cv_document_missingParagraphType: "Paragraf türünü seçiniz",
            cv_document_relationshipMarried: "Evli",
            cv_document_relationshipSingle: "Bekar",
            cv_settingsView_loading: "Yükleniyor...",
            cv_settingsView_zipcarErrorMessage: "Kod alınırken bir hata oluştu. Lütfen tekrar deneyiniz",
            cv_settingsView_validCardNumber: "Geçersiz kart numarası",
            cv_settingsView_validCardMonth: "Geçersiz son kullanma tarihi",
            cv_settingsView_validCardCvc: "Geçersiz güvenlik kodu",
            cv_settingsView_valid_sales_agreement: "cv_settingsView_valid_sales_agreement",

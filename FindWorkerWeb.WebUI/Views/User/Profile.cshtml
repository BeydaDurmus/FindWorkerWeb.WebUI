        var routes = {
            homepage: "/tr/",
            app_cv_new: "/cv-new",
            app_cv_save: "/cv/save",
            app_cv_save_pdf: "/cv/save-pdf",
            app_cv_save_from_session: "/my/cv/save-from-session"
        };

        var configurations = {
            fb_connect_scope: "public_profile,user_birthday,user_hometown,user_location,email,user_friends,user_likes",
            fb_connect_fields: "id,name,first_name,last_name,picture,gender,age_range,birthday,relationship_status,about,email,website,location,hometown,education,languages,favorite_teams,work"
        };
    </script>
    <script type="text/javascript" src="https://js.stripe.com/v2/"></script>
    <script type="text/javascript" src="~/Cv1/js/compiled/min-912f9bb.js"></script>
</body>


@using Microsoft.AspNetCore.Http;
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@addTagHelper *,Microsoft.AspNetCore.Mvc.TagHelpers
@model FindWorkerWeb.WebUI.Models.CvModel

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>Resumex - Professional HTML CSS Resume Website Template</title>

    <link href="favicon.png" rel=icon>

    <link href="https://fonts.googleapis.com/css?family=Hind:300,400,500,600,700" rel="stylesheet">

    <link href="/UserProfile/css/font-awesome.min.css" rel="stylesheet">

    <link href="/UserProfile/css/bootstrap.min.css" rel="stylesheet">

    <link href="/UserProfile/css/style.css" rel="stylesheet">
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"
            integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous">
    </script>

</head>
<body id="page-top" data-spy="scroll" data-target=".navbar">
    <form asp-controller="User" asp-action="CVData" method="post">
        <div id="main-wrapper">


            <div class="columns-block container">
                <div class="left-col-block blocks">
                    <header class="header theiaStickySidebar">
                        <div class="profile-img">
                            <img src="~/UserProfile/img/img-profile.jpg" class="img-responsive" alt="" />
                            <div class="popup photo-upload">
                                <div class="popup-title"></div>
                                <div class="popup-content">
                                    <div class="photo-action">
                                        <div class="photo-upload-btn">
                                            <input name="file" id="photo-upload-submit" type="file" class="cloudinary-fileupload photo hidden-file" data-form-data='{"upload_preset": "userphoto"}' accept="image/jpeg, image/png" data-cloudinary-field="image_id">
                                        </div>
                                        <div class="photo-delete-block">
                                            <span class="or">ya da</span>
                                            <span class="remove-photo">Fotoğrafı sil</span>
                                        </div>

                                    </div>

                                </div>
                                <div class="popup-action">
                                    <button type="button" id="user-photo-accept" class="popup-accept">Uygula</button>
                                    <button type="button" id="user-photo-cancel" class="popup-cancel">İptal</button>
                                </div>
                            </div>
                        </div>
                        <div class="content">
                            <div class="line">

                                <div class="field placeholder" contenteditable="true">Ad Soyad</div>
                                <div class="value placeholder" contenteditable="true" id="ad"> @Context.Session.GetString("Name")</div>
                            </div>
                            <div class="line">

                                <div class="field placeholder" contenteditable="true">Meslek</div>
                            </div>
                            <div class="line">

                                <div class="field placeholder" contenteditable="true">Özet Bilgi</div>

                            </div>

                            <ul class="social-icon">
                                <li><a href="#"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
                                <li><a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
                                <li><a href="#"><i class="fa fa-linkedin" aria-hidden="true"></i></a></li>
                                <li><a href="#"><i class="fa fa-slack" aria-hidden="true"></i></a></li>
                                <li><a href="#"><i class="fa fa-dribbble" aria-hidden="true"></i></a></li>
                            </ul>
                        </div>
                    </header>

                </div>

                <div class="right-col-block blocks">
                    <div class="theiaStickySidebar">
                        <section class="expertise-wrapper section-wrapper gray-bg">
                            <div class="container-fluid" name="Project">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="section-title">
                                            <h2>Projeler</h2>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <button type="button" id="projects">New Line</button>
                                    <div class="projects"></div>
                                </div>

                            </div>
                        </section>

                        <section class="section-wrapper skills-wrapper">
                            <div class="container-fluid" name="Skill">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="section-title">
                                            <div class="section-title">
                                                <h2>Yetenekler</h2>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <button type="button" id="skills">New Line</button>
                                    <div class="skills"></div>
                                </div>
                            </div>

                        </section>

                        <section class="section-wrapper skills-wrapper">
                            <div class="container-fluid" name="Language">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="section-title">
                                            <h2>Yabancı Diller</h2>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <button type="button" id="languages">New Line</button>
                                    <div class="languages"></div>
                                </div>
                            </div>
                        </section>

                        <section class="section-wrapper section-experience gray-bg">
                            <div class="container-fluid" name="WorkExperience">
                                <div class="row">
                                    <div class="col-md-12">

                                        <div class="section-title"><h2>İş Deneyimleri</h2></div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-12">

                                        <button type="button" id="experiences">New Line</button>
                                        <div class="experiences"></div>

                                    </div>
                                </div>

                            </div>

                        </section>

                        <section class="section-wrapper section-education">
                            <div class="container-fluid" name="Education">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="section-title"><h2>Eğitim</h2></div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12">

                                        <button type="button" id="educations">New Line</button>
                                        <div class="educations"></div>

                                    </div>
                                </div>

                            </div>

                        </section>

                        <section class="section-contact section-wrapper gray-bg">
                            <div class="container-fluid" name="Document">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="section-title">
                                            <h2>Sertifikalar</h2>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-12">
                                        <button type="button" id="documents">New Line</button>
                                        <div class="documents"></div>

                                    </div>
                                </div>
                            </div>
                        </section>

                        <section class="section-wrapper section-interest gray-bg">
                            <div class="container-fluid" name="Hobby">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="section-title">
                                            <h2>Hobiler</h2>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-12">
                                        <button type="button" id="hobbies">New Line</button>
                                        <div class="hobbies"></div>
                                    </div>
                                </div>

                            </div>
                        </section>

                        <section class="section-contact section-wrapper gray-bg">
                            <div class="container-fluid" name="Contact">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="section-title">
                                            <h2>İletişim</h2>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-12">
                                        <button type="button" id="contacts">New Line</button>
                                        <div class="contacts"></div>

                                    </div>
                                </div>
                            </div>
                        </section>

                        <section class="section-wrapper section-experience gray-bg">
                            <div class="container-fluid" name="Reference">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="section-title"><h2>Referanslar</h2></div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-12">
                                        <button type="button" id="references">New Line</button>
                                        <div class="references"></div>

                                    </div>
                                </div>

                            </div>


                        </section>

                        <div class="document-actions">
                            @Model.CvData
                            <div class="form-group"><label asp-for= "CvData" class=" form-control-label"> Name</label><input asp-for="CvData" type="text" id="CvName" name="CvName" placeholder="CV İsminiz.." class="form-control"></div>
                            <input asp-controller="User" asp-action="CvData" class="btn btn-md btn-primary m-0 px-3 " type="submit" value="Submit" id="save">



                        </div>



                        <div class="document-actions">
                            <button type="button" id="update" class="btn btn-md btn-primary m-0 px-3">
                                <i class="save-icon sprite">

                                </i> Güncelle
                            </button>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </form>
    <script>
        $("#references").click(function () {

            $(".references").append("<input type='text' width='30px'  placeholder='Referans Adı' name='ReferenceName' />");
            $(".references").append("<input type='text' width='30px'  placeholder='Referans Email' name='ReferencePhone' /><br/>");
            $(".references").append("<input type='text' width='30px'  placeholder='Referans Telefon' name='ReferenceEmail' />");
            $(".references").append("<input type='text' width='30px'  placeholder='Referans Pozisyonu' name='ReferencePosition' /><br/>");


        });
        $("#contacts").click(function () {

            $(".contacts").append("<input type='text' placeholder='İleşim Adı' name='ContactName' />");
            $(".contacts").append("<input type='text' placeholder='Bilgi' name='Decription' /><br/>");



        });
        $("#educations").click(function () {

            $(".educations").append("<input type='text'  width: 100px placeholder='Okul Adı' name='SchoolName' />");
            $(".educations").append("<input type='text'  width: 100px placeholder='Mezuniyet Tarihi' name='GruadetDate' /><br/>");
            $(".educations").append("<input type='text'  width: 100px placeholder='Okul Tipi' name='SchoolType' />");
            $(".educations").append("<input type='text'  width: 100px placeholder='Bölüm' name='Department' /><br/>");



        });
        $("#experiences").click(function () {

            $(".experiences").append("<input type='text' placeholder='Şirket Adı' name='CompanyName' />");
            $(".experiences").append("<input type='text' placeholder='Pozisyon' name='Position' /><br/>");
            $(".experiences").append("<input type='text' placeholder='Çalışmaya Başlama Süresi'name='WorkStartTime' />");
            $(".experiences").append("<input type='text' placeholder='Çalışma Bitiş Süresi'name='WorkFinishTime' />");
            $(".experiences").append("<input type='text' placeholder='Açıklama'name='Description' /><br/>");
        });

        $("#projects").click(function () {

            $(".projects").append("<input type='text'  size='25' placeholder='Proje Adı'  name='ProjectName'/> <br/>");
        });


        $("#skills").click(function () {

            $(".skills").append("<input type='text' placeholder='Yetenek Adı'  name='SkillName'/>");
            $(".skills").append("<input type='text' placeholder='Yetenek Seviyesi'  name='SkillName'/><br/>");
        });

        $("#languages").click(function () {

            $(".languages").append("<input type='text' placeholder='Yabancı Dil Adı'  name='LanguageName'/>");
            $(".languages").append("<input type='text' placeholder='Yabancı Dil Seviyesi'  name='LanguageLevel'/><br/>");
        });

        $("#hobbies").click(function () {

            $(".hobbies").append("<input type='text' placeholder='Hobi Adı '  name='HobbiesName '/><br/>");

        });

        $("#documents").click(function () {

            $(".documents").append("<input type='text' placeholder='Sertifika Adı'  name='DocumentationName'/>");
            $(".documents").append("<input type='text' placeholder='Açıklama'  name='DocumentDate'/><br/>");
        });
        

    </script>
    <script data-cfasync="false" src="../../../cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script>
    <script src="/UserProfile/js/jquery-2.1.4.min.js" type="e757cce0a3cddc60662fb534-text/javascript"></script>

    <script src="/UserProfile/js/bootstrap.min.js" type="e757cce0a3cddc60662fb534-text/javascript"></script>
    <script src="/UserProfile/js/theia-sticky-sidebar.js" type="e757cce0a3cddc60662fb534-text/javascript"></script>
    <script src="/UserProfile/js/scripts.js" type="e757cce0a3cddc60662fb534-text/javascript"></script>
    <script src="../../../../ajax.cloudflare.com/cdn-cgi/scripts/7089c43e/cloudflare-static/rocket-loader.min.js" data-cf-settings="e757cce0a3cddc60662fb534-|49" defer=""></script>

    <script>
        var trans = {
            cv_index_saveErrorMessage: "Kayıt esnasında bir hata oluştu. Lütfen tekrar deneyiniz.",
            cv_index_or: "ya da",
